<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.min.cs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/NormalTheme"
        android:name=".app.MyApplication">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.SnackbarActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.ScrollFlagActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.CollapsingToolbarActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ui.TabActivity"/>
        <activity android:name=".ui.BehaviorActivity"/>
        <activity android:name=".ui.ThemeControlActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
