    <EditText
        android:id="@+id/et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_rxlife"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="rxlife"/>

    <Button
        android:id="@+id/btn_takeutil"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="takeutil"/>

    <Button
        android:id="@+id/btn_takefirst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="takeutil"/>

    <Button
        android:id="@+id/btn_subscription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_subscription"/>

</LinearLayout>
